#include <iostream>
#include <math.h>
using namespace std;
void poweraA3(float a, float* b) //объявляем функцию
{
    *b = a * a * a;//высчитываем степень
}
int main(void)
{
    int i;
    for (i = 1; i <= 5; ++i) { //задаем 5 чисел для высчитывания
        float a;
        cout<<"A:";
        cin>>a;
        float b;
        poweraA3(a, &b); //вызываем функцию
        cout<<"B: "<< b<<endl << endl;
    }
    return 0;
}
